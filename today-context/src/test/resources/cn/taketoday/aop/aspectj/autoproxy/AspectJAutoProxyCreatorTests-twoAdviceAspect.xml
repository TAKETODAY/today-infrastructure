<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "https://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean class="cn.taketoday.aop.aspectj.annotation.AnnotationAwareAspectJAutoProxyCreator"/>

	<bean id="aspect" class="test.aspect.TwoAdviceAspect"/>

	<bean id="adrian" class="cn.taketoday.aop.framework.ProxyFactoryBean">
		<property name="target">
			<bean class="cn.taketoday.beans.testfixture.beans.TestBean">
				<property name="name" value="adrian"/>
				<property name="age" value="34"/>
			</bean>
		</property>
	</bean>

</beans>
