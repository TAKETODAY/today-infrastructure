<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "https://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="customEditorConfigurer" class="cn.taketoday.beans.factory.config.CustomEditorConfigurer">
		<property name="propertyEditorRegistrars">
			<bean class="cn.taketoday.jmx.export.CustomDateEditorRegistrar" />
		</property>
	</bean>

	<bean id="dateRange" class="cn.taketoday.jmx.export.DateRange">
		<property name="startDate" value="2004/10/12" />
		<property name="endDate" value="2004/11/13" />
	</bean>

	<bean class="cn.taketoday.jmx.export.MBeanExporter">
		<property name="beans">
			<map>
				<entry key="bean:name=dateRange" value-ref="dateRange" />
			</map>
		</property>
		<property name="server" ref="server" />
	</bean>

</beans>
