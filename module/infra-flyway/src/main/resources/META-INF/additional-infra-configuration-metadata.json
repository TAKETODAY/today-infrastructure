{
  "properties": [
    {
      "name": "flyway.baseline-migration-prefix",
      "defaultValue": "B",
      "description": "Filename prefix for baseline migrations. Requires Flyway Teams.",
      "deprecation": {
        "level": "error",
        "reason": "Removed in Flyway 9.0"
      }
    },
    {
      "name": "flyway.check-location",
      "type": "java.lang.Boolean",
      "deprecation": {
        "replacement": "flyway.fail-on-missing-locations",
        "level": "error"
      }
    },
    {
      "name": "flyway.cherry-pick",
      "description": "Migrations that Flyway should consider when migrating or undoing. When empty all available migrations are considered. Requires Flyway Teams.",
      "deprecation": {
        "level": "error",
        "reason": "Removed in Flyway 10"
      }
    },
    {
      "name": "flyway.clean-on-validation-error",
      "type": "java.lang.Boolean",
      "description": "Whether to automatically call clean when a validation error occurs.",
      "deprecation": {
        "level": "error",
        "reason": "Deprecated in Flyway 10.18 and removed in Flyway 11.0",
        "since": "3.4.0"
      }
    },
    {
      "name": "flyway.community-db-support-enabled",
      "defaultValue": false
    },
    {
      "name": "flyway.dry-run-output",
      "type": "java.io.OutputStream",
      "deprecation": {
        "level": "error",
        "reason": "Flyway Teams only."
      }
    },
    {
      "name": "flyway.error-handlers",
      "type": "org.flywaydb.core.api.errorhandler.ErrorHandler[]",
      "deprecation": {
        "level": "error",
        "reason": "Flyway Teams only."
      }
    },
    {
